{
  "name": {{cluster.name}},
  "description": {{cluster.description}},
  "vxnet": {{cluster.vxnet}},
  "multi_zone_policy": "round_robin",
  "advanced_actions": ["change_vxnet","scale_horizontal","resize_on_upgrade"],
  "custom_tags": ["default_deploy_method_sz", "hide_modify_instance_type_btn"],
  "backup_policy": "device",
  "incremental_backup_supported": true,
  "upgrade_policy": [
     "appv-r89epe6m",
     "appv-e97gz6wz",
     "appv-vedszzfk",
     "appv-21ddkc5h",
     "appv-315vkrx9",
     "appv-dw8hsl7o",
     "appv-7nnl5m03",
     "appv-yjt6jcur",
     "appv-htdfkqlo",
     "appv-2koqx590",
     "appv-tse5x72c"
   ],
  "nodes": [{
    "container": {
      "type": "lxc",
      "prefer_type": "lxc",
      "sriov_nic": true,
      "zone": "pek3",
      "image": "img-bx7d5uw1"
    },
    "instance_class": {{cluster.node.instance_class}},
    "count": {{cluster.node.count}},
    "cpu": {{cluster.node.cpu}},
    "memory": {{cluster.node.memory}},
    "volume": {
        "size": {{cluster.node.memory}} * 3 / 1024,
        "skip_check_size": true,
        "mount_point": "/data",
        "filesystem": "xfs"
    },
    "vertical_scaling_policy": "sequential",
    "services": {
      "start": {
        "cmd": "appctl start"
      },
      "stop": {
        "cmd": "appctl stop"
      },
      "backup": {
        "selector": "appctl findMasterNodeId",
        "nodes_to_execute_on": 1,
        "timeout": 3600,
        "cmd": "appctl backup"
      },
      "restore": {
        "cmd": "appctl restore"
      },
      "scale_in": {
        "pre_check": "appctl preScaleIn",
        "nodes_to_execute_on": 1,
        "cmd": "appctl scaleIn"
      },
      "destroy": {
        "allow_force": false,
        "cmd": "appctl destroy"
      },
      "change_vxnet": {
        "pre_check": "appctl preChangeVxnet"
      },
      "run_command": {
        "type": "custom",
        "nodes_to_execute_on": 1,
        "cmd": "appctl runCommand",
        "timeout": 1200,
        "service_params": {
          "cmd": {{service_params.cmd}},
          "db": {{service_params.db}},
          "params": {{service_params.params}},
          "timeout": {{service_params.timeout}}
        }
      },
      "add_user": {
          "type": "custom",
          "cmd": "appctl aclManage addUser",
          "timeout": 120,
          "service_params": {
              "username": {{service_params.username}},
              "passwd": {{service_params.passwd}},
              "switch": {{service_params.switch}},
              "rules": {{service_params.rules}}
          }
      },
      "set_user_rules": {
          "type": "custom",
          "cmd": "appctl aclManage setUserRules",
          "timeout": 120,
          "service_params": {
              "username": {{service_params.username}},
              "rules": {{service_params.rules}}
          }
      },
      "del_user": {
          "type": "custom",
          "cmd": "appctl aclManage delUser",
          "timeout": 120,
          "service_params": {
              "username": {{service_params.username}}
          }
      },
      "set_switch": {
          "type": "custom",
          "cmd": "appctl aclManage setSwitch",
          "timeout": 120,
          "service_params": {
              "username": {{service_params.username}},
              "switch": {{service_params.switch}}
          }
      },
      "reset_passwd": {
          "type": "custom",
          "cmd": "appctl aclManage resetPasswd",
          "timeout": 120,
          "service_params": {
              "username": {{service_params.username}},
              "passwd": {{service_params.passwd}}
          }
      },
      "get_nodes_order": {
          "event": ["upgrade", "rollback", "scale_vertical"],
          "cmd": "appctl getNodesOrder"
      },
      "restore_data_from_rdb": {
          "type": "custom",
          "cmd": "appctl restoreByCustomRdb",
          "timeout": 1200,
          "service_params": {
                "confirm": {{service_params.confirm}}
          }
      }
    }
  }],
